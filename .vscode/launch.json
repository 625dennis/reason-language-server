{
	"version": "0.2.0",
	// List of configurations. Add new configurations or edit existing ones.
	"configurations": [
		{
			"name": "Launch Client",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceRoot}/client",
				"${workspaceRoot}/example-project"
			],
			"stopOnEntry": false,
			"sourceMaps": false,
		},
		{
			"name": "Launch Native Project",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceRoot}/client",
				"${workspaceRoot}/example-native-project"
			],
			"stopOnEntry": false,
			"sourceMaps": false,
		},
		{
			"name": "Launch Self-Host",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceRoot}/client",
				"${workspaceRoot}"
			],
			"stopOnEntry": false,
			"sourceMaps": false,
		}
	]
}
